                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With IPS_QTYs AS
	(Select ISBN, SUM(Qty) AS Qty
	From IPS.dbo.IPS_DAILY 
	where Otype &lt;&gt;'Return' AND Whs = 'ING' 
	Group by ISBN)
Select ING.ISBN, CAST(ING.INGOH AS int) - IPS_QTYs.Qty AS TOTAL_QTY_ING
From TUTLIV.dbo.INGQTY ING
JOIN IPS_QTYs
ON ING.ISBN = IPS_QTYs.ISBN
Where ING.INGOH - IPS_QTYs.Qty &lt; 0;</property>
